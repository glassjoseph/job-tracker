<%= form_for [@company, @job] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :level_of_interest %>
  <%= f.number_field :level_of_interest %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :category %>
  <%= f.collection_select :category_id, Category.order(:title), :id, :title, include_blank: true%>




 <%#= f.collection_select :provider_id, Provider.order(:name),:id,:name, include_blank: true %>

  <%= f.submit %>
<% end %>
